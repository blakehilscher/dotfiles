#!/usr/bin/env ruby
# encoding: UTF-8

statement = ARGV.join(' ').split(" do ")

if statement[0] =~ /,/

  args = statement.shift.split(',')
  command = statement.join(" do ")

  args.each do |arg|
    begin
      run = command.gsub("@1", arg)
      puts run
      system( "#{run} &" )
    rescue => err
    end
  end
  
end